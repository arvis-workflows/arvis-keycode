{
  "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
  "category": "Tools",
  "creator": "Radimir Bitsov",
  "name": "arvis-keycode",
  "readme": "",
  "enabled": true,
  "version": "0.0.4",
  "description": "‚å® Arivs workflow to get JavaScript keycodes.",
  "webAddress": "https://github.com/jopemachine/arvis-keycode",
  "defaultIcon": "icon.png",
  "commands": [
    {
      "type": "scriptFilter",
      "command": "kc",
      "title": "alfred-keycode",
      "subtitle": "",
      "scriptFilter": "node index.js \"{query}\"",
      "runningSubtext": "Searching...",
      "withspace": true,
      "argType": "required",
      "actions": [
        {
          "modifiers": "normal",
          "type": "open",
          "target": "{query}"
        }
      ]
    }
  ]
}
